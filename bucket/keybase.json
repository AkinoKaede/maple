{
    "version": "5.7.0",
    "description": "The Keybase app helps you perform cryptographically-secure operations with people you know on the Internet: chatting, file sharing, even publishing public documents. It's all easier and safer with Keybase.",
    "url": "https://github.com/keybase/client",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://prerelease.keybase.io/windows/Keybase_5.7.0-20210622153854%2Be3826b703a.amd64.msi",
            "hash": "dacb6f8e0d6a3d6dc8aa680b1d5519d4c1a47a2bf28d37cf119c9b65eb75fc5e"
        }
    },
    "installer": {
        "script": "New-Item -Force -ItemType HardLink -Path \"$HOME\\AppData\\Local\\Keybase\" -Name keybase.exe -Target \"$dir\\Keybase\\keybase.exe\" | Out-Null"
    },
    "uninstaller": {
        "script": "Remove-Item \"$HOME\\AppData\\Local\\Keybase\\keybase.exe\" -Force"
    },
    "bin": [
        "Keybase\\keybase.exe",
        "Keybase\\git-remote-keybase.exe"
    ],
    "shortcuts": [
        [
            "Keybase\\Gui\\Keybase.exe",
            "Keybase"
        ]
    ],
    "checkver": {
        "url": "https://raw.githubusercontent.com/facorread/keybase-chocolatey/master/src/tools/chocolateyinstall.ps1",
        "regex": "url[ ]+= \"https:\\/\\/prerelease\\.keybase\\.io\\/windows\\/Keybase_([\\d.]+)-?(?<build>[\\S]+).amd64.msi\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://prerelease.keybase.io/windows/Keybase_$version-$matchBuild.amd64.msi",
                "hash": {
                    "url": "https://raw.githubusercontent.com/facorread/keybase-chocolatey/master/src/tools/chocolateyinstall.ps1",
                    "regex": "checksum[ ]+= \"$sha256\""
                }
            }
        }
    }
}
